# platform :ios, '9.0'

target 'IPCIndex' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for IPCIndex
  pod 'Charts'
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  pod 'RxBlocking', '~> 4.0'
  pod 'RxTest',     '~> 4.0'
  target 'IPCIndexTests' do
    inherit! :search_paths
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
    # Pods for testing
  end

  target 'IPCIndexUITests' do
    inherit! :search_paths
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
    # Pods for testing
  end

end
